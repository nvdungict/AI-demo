from tensorflow.keras.models import load_model

# Load pre-trained model from .h5 file
model = load_model('model4.h5')

# Get JSON-compatible representation of model architecture
model_json = model.to_json()

# Save JSON representation to a file
with open('pretrained_model.json', 'w') as json_file:
    json_file.write(model_json)

# Optionally, save the weights of the pre-trained model
model.save_weights('pretrained_model_weights.weights.h5')
